{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "hostname": "client",
      "image": "rashid632/multi-client",
      "essential": false
    },
    {
      "name": "server",
      "hostname": "server",
      "image": "rashid632/multi-server",
      "essential": false
    },
    {
      "name": "worker",
      "hostname": "worker",
      "image": "rashid632/multi-worker",
      "essential": false
    },
    {
      "name": "nginx",
      "hostname": "nginx",
      "image": "rashid632/multi-nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": "80",
          "containerPort": "80"
        }
      ]
    }
  ]
}